<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @editor.email %>
</p>

<p>
  <strong>Name:</strong>
  <%= @editor.name %>
</p>
<table>
  <tr>
  <strong>All articles contributed to:</strong>
  </tr>
  <% @editor.articles.each do |article|%>
  <tr>
    <td><%= article.subject %></td>
    <td><%= link_to 'Show', editor_path(current_editor) %></td>
  </tr>
  <tr></tr>
  <% end %>
</table>


<%= link_to 'Edit', edit_editor_path(@editor) %> |
<%= link_to 'Back', editors_path %>
